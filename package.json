    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@spacebudz/spacebudz-identity",
  "author": "berry_ales",
  "module": "dist/spacebudz-identity.esm.js",
  "main": "dist/spacebudz-identity.esm.js",
  "devDependencies": {
    "husky": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "lucid-cardano": "^0.6.8"
  }
}
