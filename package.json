{
  "version": "1.0.6",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "onchain",
    "offchain"
  ],
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "scripts": {
    "start": "npm run build && node dist/spacebudz-identity.esm.js",
